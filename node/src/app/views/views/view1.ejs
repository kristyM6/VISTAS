<!DOCTYPE html>
<html html lang="es">
  <head>
    <meta charset="utf-8" />
    <title>Vistas</title>
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css"
      integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <%- include('../home/vistas.ejs') %>

    <div class="row">
      <div class="col-md-6 mt-4 text-center">
        <table class="table table-striped table-bordered">
          <tr>
            <th>Usuario</th>
            <th>Ultima fecha ingreso</th>
            <th>Entrada</th>
            <th>Salida</th>
            <th>Cantidad de ingresos</th>
          </tr>
          <% for (var i = 0; i < view1.length; i ++) { %>
          <tr>
            <td><%- view1[i].Nombre_Usuario %></td>
            <td><%- view1[i].Ultima_fecha_Ingreso %></td>
            <td><%- view1[i].Entrada %></td>
            <td><%- view1[i].Salida %></td>
            <td><%- view1[i].Cantidad_Ingreso %></td>
          </tr>
          <% } %>
        </table>
      </div>
    </div>
  </body>
</html>
